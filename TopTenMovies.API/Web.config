  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IMovieService" />
        <binding name="BasicHttpBinding_IGenreService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://toptenmovies.wcf/MovieService.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMovieService" contract="MovieService.IMovieService" name="BasicHttpBinding_IMovieService" />
      <endpoint address="http://toptenmovies.wcf/GenreService.svc" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IGenreService" contract="GenreService.IGenreService" name="BasicHttpBinding_IGenreService" />
